<VirtualHost ${WEBIPSKELETON}:80>
    ServerName <DOMAIN>
    
	ErrorLog /web/_logs/<DOMAIN>_error_log
    CustomLog /web/_logs/<DOMAIN>_access_log combined

    DocumentRoot /web/<DOMAIN>/public
    
	<Directory "/web/<DOMAIN>/public">
        allow from all
        Options +Indexes
        AllowOverride ALL
    </Directory>
</VirtualHost>